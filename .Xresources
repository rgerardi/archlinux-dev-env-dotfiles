! ------------------------------------------------------------------------------
! ROFI Color theme
! ------------------------------------------------------------------------------
! Use extended color scheme
rofi.color-enabled:                  true
! Color scheme for normal row
rofi.color-normal:                   #66ffffff,#525d76,#66f5f5f5,#5294e2,#66ffffff
! Color scheme for urgent row
rofi.color-urgent:                   #ddffffff,#dc322f,#f5f5f5,#5294e2,#dc322f
! Color scheme for active row
rofi.color-active:                   #ddffffff,#c2cad0,#f5f5f5,#5294e2,#c2cad0
! Color scheme window
rofi.color-window:                   #ddffffff,#f5f5f5
! Separator style (none, dash, solid)
rofi.separator-style:                solid
